pipeline{
    agent any 
    tools{
        maven 'Maven'
    }

    stages{
        stage('git'){
            steps{
                git branch: 'main', url: 'https://github.com/ofekbarel/Project3'
            }
        }
        
        stage ('build'){
            steps{
                bat 'mvn clean install'
            }
            post{
                success{
                    echo "success ofek project build"
                }    
            }   

        }
    }
}           
